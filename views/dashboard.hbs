<h6>Dashboard</h6>
<h3>Welcome {{name}}</h3>
<p>Here are your stories</p>
{{> _messages}}

{{#if books}}
    <div class="">
           
            {{#each books}}
                <div class="row">
                    <h5>{{title}}</h5>
                    <div class="col m6">Author: {{author}}</div>
                    <div class="col m6">Genre: {{genre}}</div>
                    <p><small>{{pages}} pages</small></p>
                    {{synopsis}}
                </div>
            {{/each}}

    </div>
{{else}}
<p>You have not added any book.</p>
{{/if}}